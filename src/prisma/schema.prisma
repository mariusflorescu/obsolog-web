generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

enum Plan {
  HOBBY
}

model Tenant {
  id        String   @id
  slug      String?  @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  plan Plan @default(HOBBY)

  @@index([id, slug])
}
